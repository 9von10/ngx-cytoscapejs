{
  "name": "ngx-cytoscapejs-workspace",
  "version": "0.3.1",
  "description": "Cytoscape.js Angular Wrapper",
  "author": "https://github.com/9von10/ngx-cytoscapejs/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/9von10/ngx-cytoscapejs.git"
  },
  "bugs": {
    "url": "https://github.com/9von10/ngx-cytoscapejs/issues"
  },
  "homepage": "https://github.com/9von10/ngx-cytoscapejs#readme",
  "keywords": [
    "angular",
    "cytoscape",
    "cytoscapejs"
  ],
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "npm run serve:lib",
    "serve:lib": "ng build ngx-cytoscapejs --watch",
    "serve:demo": "ng serve demo-app",
    "serve:docs": "compodoc --serve --watch",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "ng build ngx-cytoscapejs",
    "build:demo": "ng build demo-app",
    "build:docs": "shx rm -rf docs && compodoc",
    "test": "npm run test:lib && npm run build:lib && npm run test:demo",
    "test:lib": "jest --config ./projects/ngx-cytoscapejs/jest.config.js",
    "test:demo": "jest --config ./projects/demo-app/jest.config.js",
    "lint": "ng lint",
    "format": "pretty-quick --staged",
    "release": "standard-version --no-verify",
    "gh-pages": "npm run gh-pages:build && npm run gh-pages:copy",
    "gh-pages:build": "npm run build:lib && npm run build:demo -- --base-href demo-app && npm run build:docs",
    "gh-pages:copy": "shx cp -r dist/demo-app docs && shx cp docs/index.html docs/404.html && shx cp docs/demo-app/index.html docs/demo-app/404.html"
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@js4cytoscape/cx-viz-converter": "^0.1.6",
    "cytoscape": "^3.21.1",
    "cytoscape-cx2js": "^0.6.6",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-angular": "^16.2.4",
    "@compodoc/compodoc": "^1.1.19",
    "@types/cytoscape": "^3.19.4",
    "@types/jest": "^27.5.0",
    "@types/node": "^16.11.33",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "standard-version": "^9.3.2",
    "typescript": "~4.6.2"
  }
}
